	<modelVersion>4.0.0</modelVersion>
	<groupId>com.cwa.wow</groupId>
	<version>0.0.1</version>
	<artifactId>cwa2_3001wow_message</artifactId>
	<build>
		<sourceDirectory>src</sourceDirectory>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.1</version>
				<configuration>
					<source>1.6</source>
